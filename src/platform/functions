gho(){
    PREV=$(pwd)
    cdi
    ./reinstall_and_test.sh $1 $2
    cd $PREV
}

ghh(){
    gho
    cdi
    cd misfit_toys/examples/ddp
    python ddp_driver.py
}

clr(){
    cdc
    rm -rf data
}

ghoc(){
    clr
    gho
}

cddp(){
    cdi
    cd misfit_toys/examples/ddp
}

go(){
    cddp
    gho
    python driver.py
}
