gho(){
    PREV=$(pwd)
    cdi
    source reinstall_and_test.sh $1 $2
    cd $PREV
}

ghh(){
    gho
    cdi
    cd misfit_toys/examples/ddp
    python ddp_driver.py
}

clr(){
    cdc
    rm -rf data
}

ghoc(){
    clr
    gho
}

cddp(){
    cdi
    cd misfit_toys/examples/ddp
}

go(){
    cddp
    gho
    python driver.py
}

kdata(){
    PREV=$(pwd)
    cdc
    rm -rf BENCHMARK
    cd $PREV
}

mtd(){
    dwre
    # cd /home/tyler/Documents/repos/IslandOfMisfitToys/misfit_toys/examples
    cd $(pips)
    cd site-packages/misfit_toys/examples
    python download_data.py
}

lll(){
    dwre
    rm /tmp/session.ans
    script -O /tmp/session.ans
}

rexit () {
    cact dw
    cd $(pips)
    cd site-packages/misfit_toys/examples
    python download_data.py

    exit
}

gg(){
    echo "gdown $1" | sed 's/view?.*//' | sed 's/file\/d\//uc?id=/'
}

cdn(){
    cdi misfit_toys/examples/ddp/$1
}

go(){
    # mhr
    # gho
    if [ -z $1 ]; then
        cdc
        rm -rf BENCHMARK
        cdn
        python driver.py
    fi
    if [ -z $2 ]; then
        cdn
        python dw_driver.py
    fi
    cdi debug
    python debug.py /home/tyler/protect conda/BENCHMARK/multiscale/data dw_validate/
    pfs dw_validate/ "*.txt" "*"
}

cmp_tensor(){
    DIR1=${1:-"/home/tyler/protect"}
    DIR2=${2:-"conda/BENCHMARK/multiscale/data"}
    OUT_DIR=${3:-"$ISL/debug/dw_validate/"}
    cdi debug
    python debug.py $DIR1 $DIR2 $OUT_DIR
    cd $OUT_DIR
    pfs . "*.txt" "*"
}

genp(){
    PREV=$(pwd)
    cdn
    python dw_driver.py
    cd $PREV
}

geni(){
    PREV=$(pwd)
    if [ ! -z $1 ]; then
        echo "Cleaning out BENCHMARK data"
        sleep 3
        cdc
        rm -rf BENCHMARK
    fi
    cdn
    python driver.py
    cd $PREV
}

cmarm(){
    PREV=$(pwd)
    cdc
    rm -rf data/marmousi
    cd $PREV
}

down(){
    PREV=$(pwd)
    cdi misfit_toys/examples
    python download_data.py
    cd $PREV
}