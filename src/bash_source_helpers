export VALID_BASHRC_HELPERS="$HOME/.bash_env_vars $HOME/.bash_aliases $HOME/.bash_functions $HOME/.bash_env"

IFS=$' '
for e in $VALID_BASHRC_HELPERS; do
    echo $e
    if [ -f $e ]; then
        echo "Sourcing $e -> $(readlink -f $e)"
        source $e
    else
        echo "$e not found...skipping"
    fi  
done

